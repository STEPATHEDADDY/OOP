<Window x:Class="DigitalSignature.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalSignature"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="525">
    <Grid>
        <Button x:Name="buttonOpen" Content="Open" HorizontalAlignment="Left" Margin="10,79,0,0" VerticalAlignment="Top" Width="75" Click="buttonOpen_Click"/>
        <Button x:Name="buttonPatch" Content="Patch" HorizontalAlignment="Left" Margin="90,79,0,0" VerticalAlignment="Top" Width="75" Click="buttonPatch_Click"/>
        <TextBox x:Name="textBoxPath" HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="498"/>
        <Label x:Name="labelPath" HorizontalAlignment="Left" Margin="0,-6,0,0" VerticalAlignment="Top" Width="176">
            <Label x:Name="label" Content="Path" FontSize="25"/>
        </Label>
        <Button x:Name="buttonCheck" Content="Check" HorizontalAlignment="Left" Margin="170,79,0,0" VerticalAlignment="Top" Width="75" Click="buttonCheck_Click"/>
    </Grid>
</Window>
